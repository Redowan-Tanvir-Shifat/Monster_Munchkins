<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="576.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.return_3.Controllers.tictactoe.TicTacToeMenuController">
    <Label alignment="CENTER" layoutX="291.0" layoutY="62.0" prefHeight="17.0" prefWidth="412.0"
           text="Welcome to TictacToe Game">
        <font>
          <Font size="26.0"/>
        </font>
    </Label>
    <Button fx:id="serverBtn" layoutX="392.0" layoutY="231.0" mnemonicParsing="false" onAction="#onServerBtn"
            prefWidth="218.0" text="Create a Server">
        <font>
          <Font size="20.0"/>
        </font>
    </Button>
    <Button fx:id="joinBtn" layoutX="392.0" layoutY="314.0" mnemonicParsing="false" onAction="#onJoinBtn"
            prefWidth="218.0" text="Join a Game">
        <font>
          <Font size="20.0"/>
        </font>
    </Button>
    <Button fx:id="back" layoutX="393.0" layoutY="403.0" mnemonicParsing="false" onAction="#onBack" prefWidth="218.0"
            text="Back">
        <font>
          <Font size="20.0"/>
        </font>
    </Button>
</AnchorPane>
