<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="576.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.return_3.Controllers.tictactoe.TicTacToeRoomController">
    <Label alignment="CENTER" layoutX="291.0" layoutY="62.0" prefHeight="17.0" prefWidth="412.0"
           text="TicTacToe Room">
        <font>
            <Font size="26.0"/>
        </font>
    </Label>
    <Label layoutX="291.0" layoutY="120.0" text="Server IP Address:" />
    <TextField fx:id="serverIPField" layoutX="412.0" layoutY="120.0" prefWidth="200" />
    <Label layoutX="291.0" layoutY="180.0" text="Server Port Number:" />
    <TextField fx:id="serverPortField" layoutX="412.0" layoutY="180.0" prefWidth="200" />
    <Button fx:id="joinRoomBtn" layoutX="392.0" layoutY="250.0" mnemonicParsing="false" onAction="#onJoinRoomBtn"
            prefWidth="218.0" text="Join Room">
        <font>
            <Font size="20.0"/>
        </font>
    </Button>
    <Button fx:id="backBtn" layoutX="393.0" layoutY="320.0" mnemonicParsing="false" onAction="#onBackBtn" prefWidth="218.0"
            text="Back">
        <font>
            <Font size="20.0"/>
        </font>
    </Button>
</AnchorPane>
