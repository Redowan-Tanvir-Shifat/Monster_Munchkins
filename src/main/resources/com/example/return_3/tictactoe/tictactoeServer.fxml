<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="576.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.return_3.Controllers.tictactoe.TicTacToeServerController">
    <Label alignment="CENTER" layoutX="291.0" layoutY="62.0" prefHeight="17.0" prefWidth="412.0" text="TicTacToe Server">
        <font>
            <Font size="26.0" />
        </font>
    </Label>
    <Label layoutX="291.0" layoutY="180.0" text="Port Number:" />
    <TextField fx:id="portNumberField" layoutX="412.0" layoutY="180.0" prefWidth="200" />
